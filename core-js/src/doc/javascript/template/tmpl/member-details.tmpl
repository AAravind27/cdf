<?js
var data = obj;
var self = this;
?>

<p>
    <a id="<?js= id+'_anchor' ?>" name="<?js= id+'_anchor' ?>"></a>
</p>

<table class="<?js= data.access != null ? data.access : 'public' ?> <?js= data.deprecated ? 'deprecated' : '' ?> <?js= data.inherited ? 'inherited' : '' ?>">
    <thead>
        <tr>
            <th>
                <?js= id ?> 
                <?js if(data.type) { ?>
                    : <code> <?js= self.partial('type.tmpl', data.type.names) ?> </code>
                <?js } ?>    
                <?js= self.partial('modifiers.tmpl', data) ?>  
            </th>
        </tr>    
    </thead>
    <tbody>
        <tr>
            <td>
                <p>
                <?js if(!data.description){ ?>
                    <?js= data.summary ?> 
                <?js } else { ?>
                    <?js= data.description ?>
                <?js } ?>
                </p>

                <?js= this.partial('details.tmpl', data) ?>

                <?js if (data.codeExamples) { ?>
                    <h5>Code</h5>
                    <?js= this.partial('examples.tmpl', data.codeExamples) ?>
                <?js } ?>

                <?js if (data.fires && data.fires.length) { ?>
                    <h5>Fires:</h5>
                    <ul><?js fires.forEach(function(f) { ?>
                        <li><?js= self.linkto(f) ?></li>
                    <?js }); ?></ul>
                <?js } ?>

                <?js if (data.examples && data.examples.length) { ?>
                    <h5>Example<?js= data.examples.length > 1? 's':'' ?></h5>
                    <?js= this.partial('examples.tmpl', data.examples) ?>
                <?js } ?>
            </td>
        </tr>
    </tbody>
</table>